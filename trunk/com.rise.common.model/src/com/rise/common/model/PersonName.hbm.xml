<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.rise.common.model.PersonName" table="PERSON_NAME">
		<id name="id" column="PERSON_NAME_ID">
			<generator class="assigned" />
		</id>
		<property name="dateCreated" column="DATE_CREATED" />
		<property name="dateModified" column="DATE_MODIFIED" />
		<property name="createdBy" column="CREATED_BY" />
		<property name="modifiedBy" column="MODIFIED_BY" />
		<!-- <property name="recordStatus" column="RECORD_STATUS" /> -->

		<property name="title" column="TITLE" />
		<property name="firstName" column="FIRST_NAME" />
		<property name="middleName" column="MIDDLE_NAME" />
		<property name="lastName" column="LAST_NAME" />
		<property name="suffix" column="SUFFIX" />

	</class>
</hibernate-mapping>